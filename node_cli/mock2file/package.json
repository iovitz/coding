    "type": "git",
    "url": "git+https://github.com/iovitz/mock2file.git"
  },
  "keywords": [
    "mock",
    "json",
    "xlsx",
    "excel",
    "json-to-excel",
    "create-json",
    "create-xlsx",
    "create-excel",
    "microsoft-xlsx",
    "xlsx-from-json"
  ],
  "bugs": {
